{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@canary"  
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/uploads/(.*)", "dest": "/uploads/$1" }  
  ],
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "cleanUrls": true
}
